{
  "name": "etherface",
  "version": "0.1.2",
  "private": false,
  "license": "Apache-2.0",
  "description": "Ethereum dApp UI builder",
  "keywords": [
    "ethereum",
    "dapp",
    "web3",
    "frontend",
    "blockchain"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnkozan/etherface.git"
  },
  "homepage": ".",
  "bugs": {
    "url": "https://github.com/johnkozan/etherface/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "prebuild": "npm run jsonschema",
    "test": "craco test",
    "eject": "craco eject",
    "release": "release-it",
    "prerelease": "npm run build && git status -s",
    "ghpages": "gh-pages -d build",
    "preghpages": "npm run build",
    "jsonschema": "typescript-json-schema --required --noExtraProps ./src/types/template.ts Template -o src/types/Template.json"
  },
  "module": "src/App.js",
  "bin": "bin/etherface",
  "files": [
    "build/*",
    "src/*",
    "bin/*",
    "public/*",
    "test/*"
  ],
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@craco/craco": "^5.6.4",
    "@ethersproject/providers": "^5.0.0-beta.162",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.45",
    "@material-ui/types": "^5.0.0",
    "@web3-react/core": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "ajv": "^6.12.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.19",
    "bip39": "^3.0.2",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "latest",
    "emoji-mart": "^3.0.0",
    "ethers": "^4.0.47",
    "fastify": "^2.14.1",
    "fastify-static": "^4.2.4",
    "fetch-graphql-schema": "^0.2.1",
    "graphql-tag": "^2.10.3",
    "immutable": "^4.0.0-rc.12",
    "jazzicon": "^1.5.0",
    "js-file-download": "^0.4.11",
    "notistack": "^0.9.11",
    "opn": "^6.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-files": "^2.4.8",
    "react-hooks-useform": "^0.1.66",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-toast-notifications": "^2.4.0",
    "slugify": "^1.4.0",
    "string_decoder": "^1.3.0",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "^3.8.3",
    "use-file-input": "^1.0.0",
    "use-trace-update": "^1.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@welldone-software/why-did-you-render": "welldone-software/why-did-you-render",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^2.2.0",
    "react-test-renderer": "^16.13.1",
    "release-it": "^13.6.1",
    "typescript-json-schema": "^0.42.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!use-file-input)"
    ]
  }
}
